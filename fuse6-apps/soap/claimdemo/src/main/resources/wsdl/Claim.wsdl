<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="ClaimService" targetNamespace="http://cxf.learn/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:tns="http://cxf.learn/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/">

  <wsdl:types>
